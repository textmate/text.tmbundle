	<key>command</key>
	<string>#!/bin/bash
export LC_ALL=C
while IFS= read -r line; do
  read -rn 10 prefix &lt; &lt;(tr -dc '[:alnum:]' &lt; /dev/urandom)
  printf '%s\t%s\n' "$prefix" "$line"
done|sort|cut -f2-
